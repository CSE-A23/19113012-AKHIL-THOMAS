#include <stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
    char input[100];
    int len,i;
    clrscr();
    printf("Enter string\n");
    scanf("%s",input);
    len=strlen(input);
    for(i=0;i<len;i++)
    {
        if(len>=6&&len<=20)
        {
            if(input[i]=='r')
            {
                if(input[i]=='#'||input[i]=='$'||input=='!'||input=='!')
                {
                    if(input[i]!=' ')
                    {
                        printf("Reeword\n");
                    }
                }
            }
        }
    }
    printf("Reeword\n");
    if(len<6||len>20)
    {
        printf("Nay!\n");
    }
    getch();
}
